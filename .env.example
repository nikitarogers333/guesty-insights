# Guesty API Credentials
# Get these from your Guesty Developer Console
GUESTY_CLIENT_ID=your_client_id_here
GUESTY_CLIENT_SECRET=your_client_secret_here

# Database (defaults work with docker-compose)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/guesty_insights

# Sync Configuration (optional)
SYNC_LOOKBACK_YEARS=3
SYNC_CRON_SCHEDULE=0 3 * * *

# API Configuration (optional)
API_PORT=8000
API_HOST=0.0.0.0
